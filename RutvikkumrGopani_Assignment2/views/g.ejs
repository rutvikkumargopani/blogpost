    
    
    
    <!DOCTYPE html>
    
    <html lang="en">
    
    <%- include('layout/header.ejs') -%>
        
        <body>
        
            <!-- Navigation-->
        
            <%- include('layout/navigation.ejs') -%>
    
            <!-- Page Header-->
    
            <header class="masthead" style="background-image: url('images/about-bg.jpg')">
    
                <div class="container position-relative px-4 px-lg-5">
    
                    <div class="row gx-4 gx-lg-5 justify-content-center">
    
                        <div class="col-md-10 col-lg-8 col-xl-7">
    
                            <div class="page-heading">
    
                                <h1>G Test</h1>
    
                                <span class="subheading">You can book G Test.</span>
    
                            </div>
    
                        </div>
    
                    </div>
    
                </div>
    
            </header>
    
            <!-- Main Content-->
    
            <main class="mb-4">
    
                <div class="container px-4 px-lg-5">
    
                    <div class="row gx-4 gx-lg-5 justify-content-center">
    
                        <div class="col-md-10 col-lg-8 col-xl-7">
<!--     
                            <p>DriveTest Information</p>
    
                            <div class="my-5">
    
                                <form id="contactForm" data-sb-form-api-token="API_TOKEN">
    
                                    <div class="form-floating">
    
                                        <input class="form-control" id="name" type="text" maxlength="8" placeholder="Enter your License Number..." data-sb-validations="required" pattern="[a-zA-Z0-9 ]+"/>
    
                                        <label for="name">License Number</label>
    
                                        <div class="invalid-feedback" data-sb-feedback="name:required">A License Number is required.</div>
    
    
                                    </div>
                                        <br /> -->
                                        <style>
                                            /* Style the form container */
                                            .col-md-10 {
                                                max-width: 300px;
                                                margin: 0 auto;
                                            }
                                        
                                            /* Style the heading */
                                            p {
                                                font-size: 24px;
                                                font-weight: bold;
                                                margin-bottom: 20px;
                                            }
                                        
                                            /* Style the form fields */
                                            .form-floating {
                                                margin-bottom: 20px;
                                            }
                                        
                                            /* Style the labels */
                                            label {
                                                font-weight: bold;
                                            }
                                        
                                            /* Style the input fields */
                                            .form-control {
                                                width: 100%;
                                                padding: 10px;
                                                border: 1px solid #ccc;
                                                border-radius: 5px;
                                            }
                                        
                                            /* Style the invalid feedback messages */
                                            .invalid-feedback {
                                                color: red;
                                                font-size: 14px;
                                            }
                                        </style>
                                        
                                        <div class="col-md-10 col-lg-8 col-xl-7">
                                            <p>DriveTest Information</p>
                                        
                                            <div class="my-5">
                                                <form id="contactForm" data-sb-form-api-token="API_TOKEN">
                                                    <div class="form-floating">
                                                        <input class="form-control" id="licenseNumber" type="text" maxlength="8" placeholder="Enter your License Number..." data-sb-validations="required" pattern="[a-zA-Z0-9 ]+" />
                                                        <label for="licenseNumber">License Number</label>
                                                        <div class="invalid-feedback" data-sb-feedback="licenseNumber:required">A License Number is required.</div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                        
    
                                        <div class="d-none" id="submitSuccessMessage">
    
    
                                            <div class="text-center mb-3">
    
                                                <div class="fw-bolder">Form submission successful!</div>
    
                                                To activate this form, sign up at
                                                <br />
    
                                                <a
    
                                                href="https://startbootstrap.com/solution/contact-forms">https://startbootstrap.com/solution/contact-forms</a>
    
                                            </div>
    
                                        </div>
    
                                        <!-- Submit error message-->
    
                                        <!---->
    
                                        <!-- This is what your users will see when there is-->
    
    
                                        <!-- an error submitting the form-->
                                        <div class="d-none" id="submitErrorMessage">
    
                                            <div class="text-center text-danger mb-3">Error sending message!</div>
    
                                        </div>
    
                                        <!-- Submit Button-->
    
                                        <button class="btn btn-primary text-uppercase" id="submitButton" type="submit">Submit</button>
    
                                    </form>
    
    
                                </div>
                        </div>
    
                    </div>
    
                </div>
    
            </main>


            
            <!-- Footer-->
            
            <%- include('layout/footer.ejs') -%>
            
            
            <!-- Bootstrap core JS-->
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
            
            <!-- Core theme JS-->
            
            <script src="js/scripts.js"></script>
        
